    "nedb": "^1.8.0",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "jest": "^20.0.4",
